        /**
         * Function to create star rating HTML
         * @param {number} rating - The average rating
         * @param {number} count - Number of reviews
         * @returns {string} HTML string for rating
         */
        function createRating(rating, count) {
            let stars = '';
            for (let i = 1; i <= 5; i++) {
                if (i <= Math.floor(rating)) {
                    stars += '‚òÖ';
                } else if (i - rating < 1) {
                    stars += '‚òÖ'; // You can use half-stars if desired
                } else {
                    stars += '‚òÜ';
                }
            }
            return `<span>${stars}</span> ${rating} (${count} reviews)`;
        }

        /**
         * Function to format price
         * @param {number} price 
         * @returns {string} Formatted price with currency
         */
        function formatPrice(price) {
            return price > 0 ? `¬£${price.toFixed(2)}` : 'N/A';
        }

        /**
         * Function to generate product cards
         */
        function generateProductCards() {
            const container = document.getElementById('product-container');

            products.forEach(product => {
                // Create card element
                const card = document.createElement('div');
                card.className = 'card';

                // Product Image
                const img = document.createElement('img');
                img.src = product.ProductImage ? product.ProductImage : 'https://via.placeholder.com/300x200?text=No+Image';
                img.alt = product.ProductTitle ? product.ProductTitle : 'Product Image';
                card.appendChild(img);

                // Card Content
                const cardContent = document.createElement('div');
                cardContent.className = 'card-content';

                // Product Title
                const title = document.createElement('h2');
                title.textContent = product.ProductTitle ? product.ProductTitle : 'No Title Available';
                cardContent.appendChild(title);

                // Price Section
                const priceSection = document.createElement('div');
                priceSection.className = 'price-section';

                const price = document.createElement('span');
                price.className = 'price';
                price.textContent = formatPrice(product.ProductPrice);
                priceSection.appendChild(price);

                if (product.OriginalPrice && product.OriginalPrice > product.ProductPrice) {
                    const originalPrice = document.createElement('span');
                    originalPrice.className = 'original-price';
                    originalPrice.textContent = formatPrice(product.OriginalPrice);
                    priceSection.appendChild(originalPrice);

                    const discount = document.createElement('div');
                    discount.className = 'discount';
                    discount.textContent = `-${Math.round(product.DiscountPercentage)}%`;
                    priceSection.appendChild(discount);
                }

                cardContent.appendChild(priceSection);

                // Rating
                if (product.Message.includes('‚≠ê')) {
                    const ratingDiv = document.createElement('div');
                    ratingDiv.className = 'rating';

                    // Extract rating and count from message
                    const ratingMatch = product.Message.match(/‚≠ê (\d+) Reviews: ([\d.]+) \/ 5\.0/);
                    if (ratingMatch) {
                        const count = ratingMatch[1];
                        const rating = parseFloat(ratingMatch[2]);
                        ratingDiv.innerHTML = createRating(rating, count);
                    } else {
                        ratingDiv.textContent = 'No ratings';
                    }

                    cardContent.appendChild(ratingDiv);
                }

                // Action Button
                const button = document.createElement('a');
                button.className = 'btn';
                button.href = product.ProductUrl;
                button.target = '_blank';
                button.rel = 'noopener noreferrer';
                button.textContent = 'Buy Now';
                cardContent.appendChild(button);

                card.appendChild(cardContent);
                container.appendChild(card);
            });
        }

        // Initialize the product cards on page load
        document.addEventListener('DOMContentLoaded', generateProductCards);
    </script>
</body>
</html>
